    "class Solution:\n",
    "    def factorialnumber(N):\n",
    "        ans=[]\n",
    "        fact=1\n",
    "        i=1\n",
    "        while fact<=N:\n",
    "            ans.append(fact)\n",
    "            i+=1\n",
    "            fact*=i\n",
    "        return ans\n",
    "print(Solution.factorialnumber(100))\n"
   ]
  }
