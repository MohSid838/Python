    "def binary_recursive(a,b):\n",
    "    # base case\n",
    "    if b == 0:\n",
    "        return 1\n",
    "    if b%2==1:\n",
    "        return a*binary_recursive(a,b-1)\n",
    "\n",
    "    half=binary_recursive(a,b//2)\n",
    "    return half*half\n",
    "print(binary_recursive(2,8))\n"
   ]
  }
